<h1>Edit Restaurant</h1>
    <%= form_for @restaurant do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :visit %>
    <%=f.date_select(:visit, :order => [:month, :day, :year]) %>

    <%=f.label :review%>
    <%=f.text_area :review%>

    <%=f.label :rating%>
    <%=f.select(:rating, :integer => [1, 2, 3, 4, 5])%> 

    <%= f.submit %>
<% end %> 