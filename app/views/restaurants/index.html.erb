<h1> Restaurants </h1>
<p><%= link_to 'New Restaurant', new_restaurant_path %></p>
<% @restaurants.each do |restaurant| %>
    <h2><%= restaurant.name %></h2>
    <%= link_to "Show Restaurant", restaurant_path(restaurant) %>
    <%= link_to "Edit Restaurant", edit_restaurant_path(restaurant) %>
    <%= link_to "Destroy Restaurant", restaurant, method: :delete %> 
<% end %> 


